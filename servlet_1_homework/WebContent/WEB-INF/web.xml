  <servlet-name>studentlist</servlet-name>
  <url-pattern>/studentlist.do</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>deletestudent</servlet-name>
  <servlet-class>com.bjpowernode.operation.DeleteStudent</servlet-class>
  </servlet>
